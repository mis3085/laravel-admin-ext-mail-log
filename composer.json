    "description": "Mail logs for Laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/mis3085/laravel-admin-ext-mail-log",
    "license": "MIT",
    "authors": [
